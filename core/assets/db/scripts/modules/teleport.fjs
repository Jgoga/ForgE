game.module("$teleport", function ($startPosition, $log, $screens, $LoadingScreen) {
  return {
    toStartPosition: function() {
       if ($startPosition == null) {
        throw "Could not find start position!!!";
      } else {
        this.to($startPosition);
      }
    },
    to: function(teleport) {
      $log.info("$teleport", teleport);
      $screens.set($LoadingScreen.get(teleport));
    }
  };
});
